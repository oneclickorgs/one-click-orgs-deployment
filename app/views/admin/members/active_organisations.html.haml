%p
  = link_to("All users", admin_members_path)
  |
  Active organisations
  |
  = link_to("Organisations awaiting registration", proposed_organisations_admin_members_path)

%h2 Users from active organisations
%table.users
  %thead
    %tr
      %th Name
      %th Email
      %th Organisation
      %th Last login
  - @members.each do |member|
    %tr
      %td= member.name
      %td= member.email
      %td= member.organisation_name
      %td= member.last_logged_in_at
